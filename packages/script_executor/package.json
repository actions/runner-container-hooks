{
    "name": "ml-velocity-script-executor",
    "version": "0.1.2-alpha.11",
    "dependencies": {
        "@grpc/grpc-js": "^1.12.5",
        "@grpc/proto-loader": "^0.7.13",
        "google-protobuf": "^3.21.4"
    },
    "type": "commonjs",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/google-ml-infra/runner-container-hooks.git"
    },
    "main": "dist/server.js",
    "scripts": {
        "build": "npx tsc && npx ncc build",
        "postbuild": "cp ../proto/script_executor.proto ./dist/proto/script_executor.proto",
        "start": "npm run build && node dist/index.js",
        "format": "prettier --write '**/*.ts'",
        "format-check": "prettier --check '**/*.ts'",
        "lint": "eslint src/**/*.ts"
    },
    "devDependencies": {
        "@vercel/ncc": "^0.38.3",
        "prettier": "^3.5.3",
        "typescript": "^5.0.4"
    }
}
